algoritmo modAndDiv
 definir a, b como entero;
definir residuo, cociente como entero;
 escribir"Ingresa numero 1: ";
 leer a;
 escribir"Ingresa numero 2: ";
 leer b;
residuo <- a mod b;
cociente <- a div b;

 escribir "Division entera es: ", cociente;
 escribir "Residuo es: ", residuo;
    finalgoritmo