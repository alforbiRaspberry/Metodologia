algoritmo presupuestoHospital
 definir presupuesto, presupuestoTraumatologia, presupuestoGinecologia, presupuestoPediatria como real;
 escribir "Hola Mundo ";
 leer presupuesto;

presupuestoTraumatologia <- presupuesto * 0.30;
presupuestoGinecologia <- presupuesto * 0.40;
presupuestoPediatria <- presupuesto * 0.30;

 
 escribir "Traumatologia: ", presupuestoTraumatologia;
 escribir "Ginecologia: ", presupuestoGinecologia;
 escribir "Pediatria: ", presupuestoPediatria;
       finalgoritmo